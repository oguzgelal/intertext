[
  { "cmp:Block": [ "Current: ", { "#state": "count" } ] },
  {
    "cmp:Button": "increment",
    "onClick": { "cmd:setState": ["count", { "#add": [{ "#state": "count" }, 1] }] }
  },
  {
    "cmp:Button": "decrement",
    "onClick": { "cmd:setState": ["count", { "#subtract": [{ "#state": "count" }, 1] }] }
  }
]